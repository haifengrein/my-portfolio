<svg viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <rect width="800" height="400" fill="#0a192f" rx="10" ry="10" />
  
  <!-- App interface -->
  <rect x="20" y="20" width="760" height="360" fill="#112240" rx="5" ry="5" />
  
  <!-- Dashboard header -->
  <rect x="40" y="40" width="720" height="60" fill="#1a365d" rx="5" ry="5" />
  <text x="60" y="75" font-family="Arial" font-size="18" fill="#64ffda">Current Weather</text>
  <text x="600" y="75" font-family="Arial" font-size="18" fill="#ccd6f6">New York City, NY</text>
  
  <!-- Main weather display -->
  <rect x="40" y="120" width="360" height="240" fill="#1a365d" rx="5" ry="5" />
  <text x="60" y="155" font-family="Arial" font-size="22" fill="white">Current Conditions</text>
  <text x="180" y="220" font-family="Arial" font-size="60" fill="white">23°C</text>
  <text x="150" y="260" font-family="Arial" font-size="24" fill="#a8b2d1">Partly Cloudy</text>
  
  <!-- Weather icon -->
  <circle cx="180" cy="190" r="35" fill="#ffdc42" />
  <rect x="215" y="180" width="80" height="20" fill="#e2e8f0" rx="10" ry="10" />
  
  <!-- Additional weather info -->
  <rect x="420" y="120" width="340" height="240" fill="#1a365d" rx="5" ry="5" />
  
  <!-- Weather details -->
  <text x="440" y="155" font-family="Arial" font-size="22" fill="white">Weather Details</text>
  
  <rect x="440" y="170" width="300" height="1" fill="#4a5568" />
  
  <text x="440" y="200" font-family="Arial" font-size="16" fill="#ccd6f6">Feels Like:</text>
  <text x="630" y="200" font-family="Arial" font-size="16" fill="white">25°C</text>
  
  <rect x="440" y="215" width="300" height="1" fill="#4a5568" />
  
  <text x="440" y="245" font-family="Arial" font-size="16" fill="#ccd6f6">Humidity:</text>
  <text x="630" y="245" font-family="Arial" font-size="16" fill="white">65%</text>
  
  <rect x="440" y="260" width="300" height="1" fill="#4a5568" />
  
  <text x="440" y="290" font-family="Arial" font-size="16" fill="#ccd6f6">Wind:</text>
  <text x="630" y="290" font-family="Arial" font-size="16" fill="white">12 km/h</text>
  
  <rect x="440" y="305" width="300" height="1" fill="#4a5568" />
  
  <text x="440" y="335" font-family="Arial" font-size="16" fill="#ccd6f6">Precipitation:</text>
  <text x="630" y="335" font-family="Arial" font-size="16" fill="white">10%</text>
</svg>