<svg viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <rect width="800" height="500" fill="#0a192f" rx="10" ry="10" />
  
  <!-- App interface -->
  <rect x="50" y="50" width="700" height="400" fill="#112240" rx="10" ry="10" />
  
  <!-- Current weather section -->
  <rect x="80" y="80" width="300" height="340" fill="#1a365d" rx="10" ry="10" />
  
  <!-- Weather info -->
  <text x="150" y="130" font-family="Arial" font-size="24" fill="white" text-anchor="middle">New York</text>
  <text x="150" y="160" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Monday, 10:30 AM</text>
  
  <!-- Weather icon (sun) -->
  <circle cx="150" cy="220" r="40" fill="#ffdc42" />
  
  <!-- Temperature -->
  <text x="150" y="310" font-family="Arial" font-size="48" fill="white" text-anchor="middle">23°</text>
  <text x="150" y="340" font-family="Arial" font-size="18" fill="#a8b2d1" text-anchor="middle">Sunny</text>
  
  <!-- Weather details -->
  <rect x="100" y="360" width="80" height="40" fill="#203a5d" rx="5" ry="5" />
  <text x="140" y="385" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Humidity</text>
  <text x="140" y="400" font-family="Arial" font-size="12" fill="white" text-anchor="middle">65%</text>
  
  <rect x="190" y="360" width="80" height="40" fill="#203a5d" rx="5" ry="5" />
  <text x="230" y="385" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Wind</text>
  <text x="230" y="400" font-family="Arial" font-size="12" fill="white" text-anchor="middle">12 km/h</text>
  
  <rect x="280" y="360" width="80" height="40" fill="#203a5d" rx="5" ry="5" />
  <text x="320" y="385" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Pressure</text>
  <text x="320" y="400" font-family="Arial" font-size="12" fill="white" text-anchor="middle">1015 hPa</text>
  
  <!-- 5-day forecast section -->
  <rect x="400" y="80" width="320" height="180" fill="#1a365d" rx="10" ry="10" />
  <text x="440" y="120" font-family="Arial" font-size="20" fill="white">5-Day Forecast</text>
  
  <!-- Forecast days -->
  <rect x="420" y="140" width="50" height="100" fill="#203a5d" rx="5" ry="5" />
  <text x="445" y="160" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Tue</text>
  <circle cx="445" cy="180" r="10" fill="#ffdc42" />
  <text x="445" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle">24°</text>
  
  <rect x="480" y="140" width="50" height="100" fill="#203a5d" rx="5" ry="5" />
  <text x="505" y="160" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Wed</text>
  <circle cx="505" cy="180" r="10" fill="#ffdc42" />
  <text x="505" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle">26°</text>
  
  <rect x="540" y="140" width="50" height="100" fill="#203a5d" rx="5" ry="5" />
  <text x="565" y="160" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Thu</text>
  <path d="M555,180 Q565,170 575,180 Q585,190 595,180" stroke="#a8b2d1" stroke-width="2" fill="none" />
  <text x="565" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle">22°</text>
  
  <rect x="600" y="140" width="50" height="100" fill="#203a5d" rx="5" ry="5" />
  <text x="625" y="160" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Fri</text>
  <path d="M615,180 Q625,170 635,180 Q645,190 655,180" stroke="#a8b2d1" stroke-width="2" fill="none" />
  <text x="625" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle">21°</text>
  
  <rect x="660" y="140" width="50" height="100" fill="#203a5d" rx="5" ry="5" />
  <text x="685" y="160" font-family="Arial" font-size="12" fill="#a8b2d1" text-anchor="middle">Sat</text>
  <circle cx="685" cy="180" r="10" fill="#ffdc42" />
  <text x="685" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle">25°</text>
  
  <!-- Weather map section -->
  <rect x="400" y="280" width="320" height="140" fill="#1a365d" rx="10" ry="10" />
  <text x="440" y="320" font-family="Arial" font-size="20" fill="white">Weather Map</text>
  
  <!-- Simplified map with weather data -->
  <rect x="440" y="340" width="240" height="60" fill="#203a5d" rx="5" ry="5" />
  <circle cx="510" cy="370" r="20" fill="#ffdc42" opacity="0.5" />
  <path d="M580,360 Q600,350 620,360 Q640,370 660,360" stroke="#64ffda" stroke-width="10" stroke-opacity="0.3" fill="none" />
</svg>