<svg viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <rect width="800" height="500" fill="#112240" rx="10" ry="10" />
  
  <!-- App header -->
  <rect x="20" y="20" width="760" height="60" fill="#1a365d" rx="5" ry="5" />
  <text x="40" y="55" font-family="Arial" font-size="22" fill="#64ffda">Weather Dashboard</text>
  
  <!-- Search bar -->
  <rect x="400" y="30" width="360" height="40" fill="#203a5d" rx="20" ry="20" />
  <text x="420" y="55" font-family="Arial" font-size="14" fill="#a8b2d1">Search for a location...</text>
  
  <!-- Main content area with cards -->
  <rect x="20" y="100" width="490" height="380" fill="#1a365d" rx="5" ry="5" />
  
  <!-- Current weather card -->
  <rect x="40" y="120" width="450" height="160" fill="#203a5d" rx="5" ry="5" />
  <text x="60" y="150" font-family="Arial" font-size="20" fill="white">New York City</text>
  <text x="60" y="180" font-family="Arial" font-size="40" fill="white">23°</text>
  <text x="120" y="180" font-family="Arial" font-size="16" fill="#a8b2d1">Partly Cloudy</text>
  
  <!-- Weather icons -->
  <circle cx="380" cy="160" r="30" fill="#ffdc42" />
  <path d="M440 160 Q450 130, 460 160 Q470 130, 480 160" stroke="white" stroke-width="3" fill="none" />
  
  <!-- Forecast cards -->
  <rect x="40" y="300" width="90" height="160" fill="#203a5d" rx="5" ry="5" />
  <text x="65" y="325" font-family="Arial" font-size="14" fill="white">Mon</text>
  <text x="70" y="365" font-family="Arial" font-size="20" fill="white">21°</text>
  
  <rect x="140" y="300" width="90" height="160" fill="#203a5d" rx="5" ry="5" />
  <text x="165" y="325" font-family="Arial" font-size="14" fill="white">Tue</text>
  <text x="170" y="365" font-family="Arial" font-size="20" fill="white">19°</text>
  
  <rect x="240" y="300" width="90" height="160" fill="#203a5d" rx="5" ry="5" />
  <text x="265" y="325" font-family="Arial" font-size="14" fill="white">Wed</text>
  <text x="270" y="365" font-family="Arial" font-size="20" fill="white">24°</text>
  
  <rect x="340" y="300" width="90" height="160" fill="#203a5d" rx="5" ry="5" />
  <text x="365" y="325" font-family="Arial" font-size="14" fill="white">Thu</text>
  <text x="370" y="365" font-family="Arial" font-size="20" fill="white">22°</text>
  
  <!-- Weather map -->
  <rect x="530" y="100" width="250" height="250" fill="#1a365d" rx="5" ry="5" />
  <ellipse cx="655" cy="225" rx="100" ry="100" fill="#203a5d" />
  <path d="M605 180 Q655 150, 705 180 Q730 210, 705 240 Q655 270, 605 240 Q580 210, 605 180" fill="#386fa4" opacity="0.7" />
  <text x="610" y="130" font-family="Arial" font-size="16" fill="white">Weather Map</text>
  
  <!-- Details section -->
  <rect x="530" y="370" width="250" height="110" fill="#1a365d" rx="5" ry="5" />
  <text x="550" y="395" font-family="Arial" font-size="14" fill="white">Humidity: 65%</text>
  <text x="550" y="425" font-family="Arial" font-size="14" fill="white">Wind: 12 km/h</text>
  <text x="550" y="455" font-family="Arial" font-size="14" fill="white">Precipitation: 10%</text>
</svg>