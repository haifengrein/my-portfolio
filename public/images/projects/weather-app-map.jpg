<svg viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="400" fill="#0a192f" rx="10" ry="10" />
  
  <!-- Map container -->
  <rect x="20" y="20" width="760" height="360" fill="#112240" rx="5" ry="5" />
  
  <!-- Map header -->
  <rect x="40" y="40" width="720" height="40" fill="#1a365d" rx="5" ry="5" />
  <text x="60" y="65" font-family="Arial" font-size="16" fill="#64ffda">Interactive Weather Map</text>
  
  <!-- Map layer controls -->
  <rect x="400" y="45" width="80" height="30" fill="#203a5d" rx="15" ry="15" />
  <text x="415" y="65" font-family="Arial" font-size="12" fill="#a8b2d1">Temperature</text>
  
  <rect x="490" y="45" width="80" height="30" fill="#64ffda" rx="15" ry="15" />
  <text x="505" y="65" font-family="Arial" font-size="12" fill="#112240">Precipitation</text>
  
  <rect x="580" y="45" width="80" height="30" fill="#203a5d" rx="15" ry="15" />
  <text x="605" y="65" font-family="Arial" font-size="12" fill="#a8b2d1">Wind</text>
  
  <!-- Map area -->
  <rect x="40" y="100" width="720" height="260" fill="#203a5d" rx="5" ry="5" />
  
  <!-- Map content -->
  <!-- Landmass shapes -->
  <path d="M100,200 C150,180 200,220 250,200 C300,180 350,190 400,210 C450,230 500,220 550,190 C600,160 650,190 700,180 L700,300 L100,300 Z" fill="#1a365d" />
  
  <!-- Water -->
  <path d="M100,200 C150,180 200,220 250,200 C300,180 350,190 400,210 C450,230 500,220 550,190 C600,160 650,190 700,180 L700,100 L100,100 Z" fill="#0c4a6e" />
  
  <!-- Precipitation overlay -->
  <path d="M200,150 C250,140 280,170 330,150 C380,130 450,140 500,150 C550,160 600,140 650,160" fill="none" stroke="#64ffda" stroke-width="40" stroke-opacity="0.3" />
  
  <!-- City markers -->
  <circle cx="250" cy="220" r="5" fill="white" />
  <text x="260" y="225" font-family="Arial" font-size="12" fill="white">New York</text>
  
  <circle cx="450" cy="180" r="5" fill="white" />
  <text x="460" y="185" font-family="Arial" font-size="12" fill="white">Chicago</text>
  
  <circle cx="600" cy="230" r="5" fill="white" />
  <text x="610" y="235" font-family="Arial" font-size="12" fill="white">Denver</text>
  
  <!-- Temperature legend -->
  <rect x="40" y="320" width="150" height="20" rx="3" ry="3">
    <linearGradient id="tempGradient" x1="0%" y1="0%" x2="100%" y1="0%">
      <stop offset="0%" stop-color="#0c4a6e" />
      <stop offset="50%" stop-color="#64ffda" />
      <stop offset="100%" stop-color="#ef4444" />
    </linearGradient>
    <animate attributeName="fill" values="#0c4a6e;url(#tempGradient)" dur="0.5s" fill="freeze" />
  </rect>
  <text x="40" y="355" font-family="Arial" font-size="10" fill="#a8b2d1">0°C</text>
  <text x="115" y="355" font-family="Arial" font-size="10" fill="#a8b2d1">15°C</text>
  <text x="190" y="355" font-family="Arial" font-size="10" fill="#a8b2d1">30°C</text>
</svg>